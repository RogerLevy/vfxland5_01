empty

class: abc 
    is-a %totem 
    prop a <save 
    prop b <save 
    prop c <save 
    default-dir: abc
    extension: abc
    format: 1
class;

\ abc +totem test.abc [[ 1 a ! 2 b ! 3 c ! ]] 
\ test.abc keep

\ abc +totem test2.abc [[ 4 a ! 5 b ! 6 c ! ]] 
\ test2.abc keep


s" abc" abc load-default-dir
test.abc [[ a ? b ? c ? ]]
test2.abc [[ a ? b ? c ? ]]
