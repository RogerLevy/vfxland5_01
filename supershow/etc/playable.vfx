trait: playable
    prop playhead <fixed <save  \ seconds
    prop playing
    prop spd <fixed <save
    :: play ( - ) stub ;
    :: stop ( - ) stub ;
    :: pause ( - ) stub ;
    :: resume ( - ) stub ;
    :: playing! ( flag - ) 
        (playing) !  
        (playing) @ if resume else pause then ;
    \ :: playing? ( - flag ) (playing) @ ;
trait;