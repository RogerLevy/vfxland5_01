
{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build-engineer",
            "type": "shell",
            "command": "cmd",
            "args": ["/c", "start", "cmd", "/c", "build.bat"],
            "options": {
                "cwd": "${workspaceFolder}/engineer"
            },
            "group": "build"
        },
        {
            "label": "launch-engineer",
            "type": "shell",
            "command": "cmd",
            "args": [
                "/c",
                "start",
                "cmd",
                "/c",
                "dev.bat"
            ],
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "build"
        }

    ]
}
